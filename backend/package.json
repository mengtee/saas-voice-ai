{
  "name": "customer-service-backend",
  "version": "1.0.0",
  "description": "Backend service for integrating Twilio and Eleven Labs for voice calls",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "migrate:create": "npx db-migrate create",
    "migrate:up": "npx db-migrate up",
    "migrate:down": "npx db-migrate down -c",
    "migrate:clean": "npx db-migrate down -c 100",
    "migrate:seed": "ts-node ./src/seeders/seed.ts",
    "migrate:reset": "npm run migrate:clean && npm run migrate:up",
    "migrate:reset:seed": "npm run migrate:clean && npm run migrate:up && npm run migrate:seed"
  },
  "dependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/pg": "^8.15.5",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "twilio": "^4.19.0",
    "ws": "^8.14.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "twilio",
    "elevenlabs",
    "voice",
    "ai",
    "customer-service"
  ],
  "author": "",
  "license": "MIT"
}
